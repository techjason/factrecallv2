{
  "cSpell.words": [
    "CAIE",
    "Edexcel",
    "factrecall",
    "fumadocs",
    "lucide",
    "Millatt",
    "WJEC"
  ],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "markdown.preview.breaks": true,
  "markdown.extension.toc.updateOnSave": true
}
